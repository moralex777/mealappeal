{
  "name": "mealappeal",
  "version": "0.1.0",
  "description": "Smart nutrition analysis SaaS with mobile-first PWA experience",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3004",
    "build": "next build",
    "start": "next start -p 3004",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "lint:fix": "next lint --fix",
    "validate": "npm run lint && npm run format:check && npm run typecheck",
    "setup": "node tools/setup-environment.js",
    "clean": "node tools/cleanup.js",
    "clean:preview": "node tools/cleanup.js --dry-run",
    "security:scan": "node tools/credential-scanner.js",
    "dev:reset": "npm run clean && npm run setup",
    "test:all": "echo 'Running comprehensive tests...' && find scripts/test -name '*.js' -exec node {} \\;",
    "db:validate": "node scripts/db/check-actual-schema.js",
    "debug:login": "node scripts/dev/debug-login.js",
    "debug:signup": "node scripts/dev/quick-signup.js",
    "validate:env": "node scripts/dev/validate-environment.js",
    "health:check": "curl -f http://localhost:3004/api/health || echo 'Health check failed - ensure server is running'",
    "check:branch": "node scripts/dev/check-branch-name.js",
    "setup:stripe": "node scripts/dev/setup-stripe-dev.js",
    "fix:stripe": "node scripts/dev/fix-stripe-env.js",
    "stripe:products": "node scripts/dev/create-stripe-products.js",
    "test:ingredients": "node scripts/test/test-ingredient-saving.js",
    "test:dates": "echo 'Run test-share-reset-date-readonly.sql in Supabase SQL editor'",
    "test:integration": "npm run test:ingredients",
    "test:safe": "NODE_ENV=test npm run test:integration",
    "test:image-storage": "node scripts/test/test-image-storage.js",
    "predeploy": "node scripts/test/pre-deploy-checks.js",
    "check:ai-updates": "node scripts/maintenance/check-ai-updates.js",
    "weekly:maintenance": "npm run check:ai-updates && npm run test:all",
    "admin:list": "node scripts/maintenance/manage-admins.js list",
    "admin:add": "node scripts/maintenance/manage-admins.js add",
    "admin:remove": "node scripts/maintenance/manage-admins.js remove",
    "admin:review": "node scripts/maintenance/weekly-admin-review.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
